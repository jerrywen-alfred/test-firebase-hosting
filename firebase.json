{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appAssociation": "AUTO",
    "rewrites": [
      {
        "source": "/p/**",
        "dynamicLinks": true
      },
      {
        "source": "/premium/**",
        "dynamicLinks": true
      },
      {
        "source": "/store/**",
        "dynamicLinks": true
      }
    ]
  }
}